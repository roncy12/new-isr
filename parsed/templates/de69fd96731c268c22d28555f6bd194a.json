{
  "components/common/header": "<a href=\"#main-content\" class=\"skip-to-main-link\">{{lang 'header.skip_to_main'}}</a>\n{{#if banners.top}}\n<div class=\"banners\" data-banner-location=\"top\">\n    {{#each (limit banners.top_metadata 1)}}\n    <div class=\"banner\" data-event-type=\"promotion\" data-entity-id=\"{{this.id}}\" data-name=\"{{this.banner-name}}\" data-position=\"{{this.location}}\" data-banner-id=\"{{this.banner-name}}\">\n        <div data-event-type=\"promotion-click\">\n            {{{this.content}}}\n        </div>\n    </div>\n    {{/each}}\n</div>\n{{/if}}\n<header class=\"header\" id=\"myHeader\" role=\"banner\" >\n    <!-- <div class=\"top-header\">\n        <nav class=\"container\">\n            <div class=\"top-deals\">\n                    <p class=\"dealofweek\"><img  style=\"margin-right:7px;\" src=\"https://cdn11.bigcommerce.com/s-tpv42kg/content/home/deals.png\" alt=\"\"><a href=\"https://www.kitchenessentials.ca/deals-of-the-week/\">Deal's of the Week</a></p>\n            </div>\n            <div class=\"top-shipping\">\n                            <img style=\"margin-right:7px;\" class=\"__mce_add_custom__\" title=\"free-shiping.png\" src=\"https://cdn11.bigcommerce.com/s-tpv42kg/content/home/shipping2.png\" alt=\"free-shiping.png\" width=\"32\" height=\"18\">\n                            <a href=\"https://www.kitchenessentials.ca/shipping-returns\">Free Shipping on Orders Over $75</a>\n                \n            </div>\n            <div class=\"top-custom-link\">\n                <a href=\"https://www.verified-reviews.com/reviews/kitchenessentials.ca\">Reviews </a>  \n                |\n                <a href=\"/brands/\">Brands</a>\n                |   \n                <a href=\"/contact-us/\">Contact Us </a>\n            </div>\n            \n        </nav>\n    </div> -->\n\n    <a href=\"#\" class=\"mobileMenu-toggle\" data-mobile-menu-toggle=\"menu\">\n        <span class=\"mobileMenu-toggleIcon\">Toggle menu</span>\n    </a>\n\n    <div class=\"head-container\">\n\n        <div>\n            {{#if page_type '===' 'default'}}\n                <h1 class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n                    {{> components/common/store-logo}}\n                </h1>\n            {{else}}\n                <div class=\"header-logo header-logo--{{theme_settings.logo-position}}\">\n                    {{> components/common/store-logo}}\n                </div>\n            {{/if}}\n        </div>\n\n        <div class=\"navPages-container\" id=\"menu\" data-menu>\n            {{> aoc-components/components/common/navigation-menu}}\n        </div>\n\n        <div class=\"navuser-right\">{{> aoc-components/components/common/navigation}}</div>\n    </div>\n</header>\n\n{{{region name=\"header_bottom--global\"}}}\n{{{region name=\"header_bottom\"}}}\n",
  "components/common/store-logo": "<a href=\"{{urls.home}}\" class=\"header-logo__link\">\n    {{#if settings.store_logo.image}}\n        {{#if theme_settings.logo_size '===' 'original'}}\n            <img class=\"header-logo-image-unknown-size\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n        {{else}}\n            <div class=\"header-logo-image-container\">\n                <img class=\"header-logo-image\" src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n            </div>\n        {{/if}}\n    {{else}}\n        <span class=\"header-logo-text\">{{settings.store_logo.title}}</span>\n    {{/if}}\n</a>\n",
  "aoc-components/components/common/navigation-menu": "<nav class=\"navPages\">\r\n    <div class=\"navPages-quickSearch\">\r\n        {{> components/common/quick-search name='nav-menu-quick-search'}}\r\n    </div>\r\n    <ul class=\"navPages-list{{#if theme_settings.navigation_design '!==' 'simple'}} navPages-list-depth-max{{/if}}\">\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/green-tea-memory-foam-bed-mattres/\"\r\n               aria-label=\"{{name}}\">\r\n                Shop Our Mattress&nbsp;\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/sleep-trial/\"\r\n               aria-label=\"{{name}}\">\r\n                Sleep Trial&nbsp;\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/the-truth/\"\r\n               aria-label=\"{{name}}\">\r\n                The Truth&nbsp;\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/about-us/\"\r\n               aria-label=\"{{name}}\">\r\n                About Us&nbsp;\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/reviews/\"\r\n               aria-label=\"{{name}}\">\r\n                Reviews&nbsp;\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/support/\"\r\n               aria-label=\"{{name}}\">\r\n                Support&nbsp;\r\n            </a>\r\n        </li>\r\n        <li class=\"navPages-item navPages-item-page\">\r\n            <a class=\"navPages-action \"\r\n               href=\"/blog/\"\r\n               aria-label=\"{{name}}\">\r\n                Blog&nbsp;\r\n            </a>\r\n        </li>\r\n    </ul>\r\n    <ul class=\"navPages-list navPages-list--user\">\r\n        {{#if currency_selector.currencies.length '>' 1}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu\"\r\n                   href=\"#\"\r\n                   data-collapsible=\"navPages-currency\"\r\n                   aria-controls=\"navPages-currency\"\r\n                   aria-expanded=\"false\"\r\n                   aria-label=\"{{lang 'common.currency' code=currency_selector.active_currency_code}}\"\r\n                >\r\n                    {{lang 'common.currency' code=currency_selector.active_currency_code}}\r\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg>\r\n                            <use xmlns:xlink=\"http://www.w3.org/1999/xlink\" xlink:href=\"#icon-chevron-down\"></use>\r\n                        </svg>\r\n                    </i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-currency\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        {{#each currency_selector.currencies}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{{switch_url}}}\"\r\n                               aria-label=\"{{name}}\"\r\n                               data-currency-code=\"{{{code}}}\"\r\n                               {{#if cart_currency_switch_url.length '>' 0 }}\r\n                               data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\r\n                               {{/if}}\r\n                               data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\r\n                            >\r\n                                {{#if is_active}}\r\n                                    <strong>{{name}}</strong>\r\n                                {{else}}\r\n                                    {{name}}\r\n                                {{/if}}\r\n                            </a>\r\n                        </li>\r\n                        {{/each}}\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n        {{/if}}\r\n        {{#if settings.gift_certificates_enabled}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.gift_certificate.purchase}}\"\r\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\r\n                >\r\n                    {{lang 'common.gift_cert'}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if customer.store_credit.value '>' 0}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--storeCredit\">\r\n                    {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.product_list_display_mode '==' 'list'}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action navPages-action--compare\"\r\n                   href=\"{{urls.compare}}\"\r\n                   data-compare-nav\r\n                   aria-label=\"{{lang 'common.compare'}}\"\r\n                >\r\n                    {{lang 'common.compare'}}\r\n                    <span class=\"countPill countPill--positive countPill--alt\"></span>\r\n                </a>\r\n            </li>\r\n        {{/if}}\r\n        {{#if customer}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action has-subMenu\"\r\n                   href=\"{{urls.account.index}}\"\r\n                   data-collapsible=\"navPages-account\"\r\n                   aria-label=\"{{lang 'common.account'}}\"\r\n                >\r\n                    {{lang 'common.account'}}\r\n                    <i class=\"icon navPages-action-moreIcon\" aria-hidden=\"true\">\r\n                        <svg><use xlink:href=\"#icon-chevron-down\" /></svg>\r\n                    </i>\r\n                </a>\r\n                <div class=\"navPage-subMenu\" id=\"navPages-account\" aria-hidden=\"true\" tabindex=\"-1\">\r\n                    <ul class=\"navPage-subMenu-list\">\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.orders.all}}\"\r\n                               aria-label=\"{{lang 'account.nav.orders'}}\"\r\n                            >\r\n                                {{lang 'account.nav.orders'}}\r\n                            </a>\r\n                        </li>\r\n                        {{#if settings.returns_enabled}}\r\n                            <li class=\"navPage-subMenu-item\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\"\r\n                                   href=\"{{urls.account.returns}}\"\r\n                                   aria-label=\"{{lang 'account.nav.returns'}}\"\r\n                                >\r\n                                    {{lang 'account.nav.returns'}}\r\n                                </a>\r\n                            </li>\r\n                        {{/if}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.inbox}}\"\r\n                               aria-label=\"{{lang 'account.mobile_nav.messages'}}\"\r\n                            >\r\n                                {{lang 'account.mobile_nav.messages'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.addresses}}\"\r\n                               aria-label=\"{{lang 'account.nav.addresses'}}\"\r\n                            >\r\n                                {{lang 'account.nav.addresses'}}\r\n                            </a>\r\n                        </li>\r\n                        {{#if settings.show_payment_methods}}\r\n                            <li class=\"navPage-subMenu-item\">\r\n                                <a class=\"navPage-subMenu-action navPages-action\"\r\n                                   href=\"{{urls.account.payment_methods.all}}\"\r\n                                   aria-label=\"{{lang 'account.nav.payment_methods'}}\"\r\n                                >\r\n                                    {{lang 'account.nav.payment_methods'}}\r\n                                </a>\r\n                            </li>\r\n                        {{/if}}\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.wishlists.all}}\"\r\n                               aria-label=\"{{lang 'account.mobile_nav.wishlists'}}\"\r\n                            >\r\n                                {{lang 'account.mobile_nav.wishlists'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.recent_items}}\"\r\n                               aria-label=\"{{lang 'account.nav.recently_viewed'}}\"\r\n                            >\r\n                                {{lang 'account.nav.recently_viewed'}}\r\n                            </a>\r\n                        </li>\r\n                        <li class=\"navPage-subMenu-item\">\r\n                            <a class=\"navPage-subMenu-action navPages-action\"\r\n                               href=\"{{urls.account.details}}\"\r\n                               aria-label=\"{{lang 'account.nav.settings'}}\"\r\n                            >\r\n                                {{lang 'account.nav.settings'}}\r\n                            </a>\r\n                        </li>\r\n                    </ul>\r\n                </div>\r\n            </li>\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.auth.logout}}\"\r\n                   aria-label=\"{{lang 'common.logout'}}\"\r\n                >\r\n                    {{lang 'common.logout'}}\r\n                </a>\r\n            </li>\r\n        {{else}}\r\n            <li class=\"navPages-item\">\r\n                <a class=\"navPages-action\"\r\n                   href=\"{{urls.auth.login}}\"\r\n                   aria-label=\"{{lang 'common.login'}}\"\r\n                >\r\n                    {{lang 'common.login'}}\r\n                </a>\r\n                {{#if settings.account_creation_enabled}}\r\n                    {{lang 'common.or'}}\r\n                    <a class=\"navPages-action\"\r\n                       href=\"{{urls.auth.create_account}}\"\r\n                       aria-label=\"{{lang 'common.sign_up'}}\"\r\n                    >\r\n                        {{lang 'common.sign_up'}}\r\n                    </a>\r\n                {{/if}}\r\n            </li>\r\n        {{/if}}\r\n        {{#if theme_settings.social_icon_placement_top}}\r\n            <li class=\"navPages-item\">\r\n                {{> components/common/social-links}}\r\n            </li>\r\n        {{/if}}\r\n    </ul>\r\n</nav>\r\n",
  "aoc-components/components/common/navigation": "<nav class=\"navUser\">\n    {{> components/common/currency-selector}}\n\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\"\n                   aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\n                >\n                    <span class=\"navUser-action-divider\">\n                        {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                    </span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action navUser-item--compare\"\n               href=\"{{urls.compare}}\"\n               data-compare-nav\n               aria-label=\"{{lang 'common.compare'}}\"\n            >\n                {{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span>\n            </a>\n        </li>\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navUser-item navUser-item--social\">\n                {{> components/common/social-links}}\n            </li>\n            <li class=\"navUser-item navUser-item--divider\">|</li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <button class=\"navUser-action navUser-action--quickSearch\"\n               type=\"button\"\n               id=\"quick-search-expand\"\n               data-search=\"quickSearch\"\n               aria-controls=\"quickSearch\"\n               aria-label=\"{{lang 'common.search'}}\"\n            >\n                <!-- {{lang 'common.search'}} -->\n                <img class=\"lazyload\" src=\"{{cdn 'https://cdn11.bigcommerce.com/s-z2qdisybty/product_images/uploaded_images/search-icon.png?t=1626915996'}}\" />\n            </button>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    {{lang 'common.gift_cert'}}\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--account\">\n            {{#if customer}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.account.index}}\"\n                   aria-label=\"{{lang 'common.account'}}\"\n                >\n                    {{lang 'common.account'}}\n                </a>\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.logout}}\"\n                   aria-label=\"{{lang 'common.logout'}}\"\n                >\n                    {{lang 'common.logout'}}\n                </a>\n            {{else}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.login}}\"\n                   aria-label=\"{{lang 'common.login'}}\"\n                >\n                    <!-- {{lang 'common.login'}} -->\n                    <img class=\"lazyload\" src=\"{{cdn 'https://cdn11.bigcommerce.com/s-z2qdisybty/product_images/uploaded_images/account-icon-header.png?t=1626915998'}}\" />\n                </a>\n                <!-- {{#if settings.account_creation_enabled}}\n                    <span class=\"navUser-or\">{{lang 'common.or'}}</span>\n                    <a class=\"navUser-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}} -->\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a class=\"navUser-action\"\n               data-cart-preview\n               data-dropdown=\"cart-preview-dropdown\"\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n                <span class=\"navUser-item-cartLabel\">\n                    <img class=\"lazyload\" src=\"{{cdn 'https://cdn11.bigcommerce.com/s-z2qdisybty/product_images/uploaded_images/cart-icon.png?t=1626915999'}}\" />\n                    <!-- {{lang 'common.cart'}} -->\n                </span>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" data-prevent-quick-search-close>\n        {{> components/common/quick-search name='nav-quick-search'}}\n    </div>\n</nav>\n",
  "components/common/social-links": "{{#if social_media}}\n    <ul class=\"socialLinks socialLinks--alt\">\n        {{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\">\n                    <svg><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n{{/if}}\n",
  "components/common/currency-selector": "{{#if currency_selector.currencies.length '>' 1}}\n<ul class=\"navUser-section\">\n    <li class=\"navUser-item\">\n        <a class=\"navUser-action navUser-action--currencySelector has-dropdown\"\n           href=\"#\"\n           data-dropdown=\"currencySelection\"\n           aria-controls=\"currencySelection\"\n           aria-expanded=\"false\"\n        >\n            {{lang 'common.currency' code=currency_selector.active_currency_code}}\n            <i class=\"icon\" aria-hidden=\"true\">\n                <svg>\n                    <use xlink:href=\"#icon-chevron-down\" />\n                </svg>\n            </i>\n        </a>\n        <ul class=\"dropdown-menu\" id=\"currencySelection\" data-dropdown-content aria-hidden=\"true\" tabindex=\"-1\">\n            {{#each currency_selector.currencies}}\n            <li class=\"dropdown-menu-item\">\n                <a href=\"{{{switch_url}}}\"\n                   data-currency-code=\"{{{code}}}\"\n                   {{#if cart_currency_switch_url.length '>' 0 }}\n                   data-cart-currency-switch-url=\"{{{cart_currency_switch_url}}}\"\n                   {{/if}}\n                   data-warning=\"{{lang 'common.currency_switch_promotion'}}\"\n                >\n                {{#if is_active}}\n                    <strong>{{name}}</strong>\n                {{else}}\n                    {{name}}\n                {{/if}}\n                </a>\n            </li>\n            {{/each}}\n        </ul>\n    </li>\n</ul>\n{{/if}}\n",
  "components/common/quick-search": "<div class=\"container\">\n    <form class=\"form\" onsubmit=\"return false\" data-url=\"{{urls.search}}\" data-quick-search-form>\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n                <label class=\"is-srOnly\" for=\"{{name}}\">{{lang \"search.quick_search.input_label\"}}</label>\n                <input class=\"form-input\"\n                       data-search-quick\n                       name=\"{{name}}\"\n                       id=\"{{name}}\"\n                       data-error-message=\"{{lang 'search.error.empty_field'}}\"\n                       placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\"\n                       autocomplete=\"off\"\n                >\n            </div>\n        </fieldset>\n    </form>\n    <section class=\"quickSearchResults\" data-bind=\"html: results\"></section>\n    <p role=\"status\"\n       aria-live=\"polite\"\n       class=\"aria-description--hidden\"\n       data-search-aria-message-predefined-text=\"{{{lang 'search.results.quick_count_live'}}}\"\n    ></p>\n</div>\n"
}